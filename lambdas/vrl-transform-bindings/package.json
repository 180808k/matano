{
  "name": "@matano/vrl-transform-bindings",
  "version": "1.0.0",
  "description": "Rust bindings to VRL for transforming unstructured log to structured events, and utilities for serializing to Kafka messages.",
  "main": "ts/index.js",
  "files": [
    "ts"
  ],
  "scripts": {
    "build": "cargo-cp-artifact -nc ts/functions.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "npm run build-release",
    "postinstall": "tsc ts/*.ts --outDir ts",
    "test": "cargo test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cargo-cp-artifact": "^0.1"
  }
}
